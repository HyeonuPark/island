{
    "compilerOptions": {
        "module": "es2015",
        "target": "es5",
        "lib":[
            "es6",
            "dom"
        ],
        "jsx": "react",
        "moduleResolution": "node",
        "rootDir": "src",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noUnusedLocals": true
    },
    "include": [
        "src"
    ]
}
